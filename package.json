    "@vue/cli-plugin-eslint": "^4.5.18",
    "@vue/cli-service": "^4.5.18",
    "@vue/eslint-config-prettier": "6.0.0",
    "babel-eslint": "10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "1.19.1",
    "sass": "1.32.13",
    "sass-loader": "10.2.0",
    "vue-cli-plugin-vuetify": "2.4.7",
    "vuetify-loader": "1.7.3"
  },
  "engines": {
    "node": ">=10.13.0",
    "npm": ">= 5.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "generator-star-spacing": "off",
      "no-tabs": "off",
      "no-unused-vars": "off",
      "no-console": "off",
      "no-irregular-whitespace": "off",
      "no-debugger": "off",
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
