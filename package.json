{
  "name": "Fzshuai-Weblog",
  "version": "3.0.0",
  "description": "Fzshuai-Weblog 前台 UI 工程",
  "author": "fzshuai",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build",
    "preview": "node build/index.js --preview",
    "lint": "vue-cli-service lint"
  },
  "keywords": [
    "vue",
    "blog",
    "dashboard",
    "element-ui",
    "boilerplate"
