    "axios": "^1.3.6",
    "clipboard": "2.0.11",
    "core-js": "3.26.0",
    "dayjs": "^1.11.7",
    "easy-typer-js": "^2.1.0",
    "highlight.js": "10.7.3",
    "jquery": "^3.6.0",
    "markdown-it": "12.3.2",
    "nprogress": "0.2.0",
    "recorderx": "2.0.2",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "tocbot": "4.25.0",
    "vue": "^2.7.16",
    "vue-avatar-cropper": "^1.0.9",
    "vue-axios": "^2.1.5",
    "vue-baberrage": "^3.2.4",
    "vue-cute-timeline": "^1.2.8",
    "vue-image-swipe": "^1.0.5",
    "vue-infinite-loading": "^2.4.5",
